{
    "compileOnSave": true,
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true
    },
    "filesGlob": [
        "./**/*.ts",
        "!./node_modules/**/*.ts"
    ],
    "files": [
        "./cssClassAddRemove.ts",
        "./classes.ts",
        "./data.ts",
        "./fall15Hours.ts",
        "./functions.ts",
        "./spring15Hours.ts",
        "./summer15Hours.ts"
    ],
    "atom": {
        "rewriteTsconfig": true
    }
}
